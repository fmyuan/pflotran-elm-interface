    LIQUID_PRESSURE HYDROSTATIC
    TEMPERATURE DIRICHLET
  END
  LIQUID_PRESSURE 101325.d0
  DATUM 0.0d0 0.0d0 -0.1d0
  TEMPERATURE 21.6
  END



#=========================== condition couplers ===============================
# initial condition
INITIAL_CONDITION
  TRANSPORT_CONDITION initial
  FLOW_CONDITION initial
  REGION all
END

