#=========================== solver options ===================================
NUMERICAL_METHODS TRANSPORT
! For single point sim, SOLVER DIRECT and other options commented out seems to work the best 
LINEAR_SOLVER
  SOLVER DIRECT
  !SOLVER ITERATIVE     ! default: ITERATIVE or KRYLOV: KSP_TYPE of BCGS and PC_TYPE of BJACOBI, if not modified below
  !PC_TYPE JACOBI       ! default (iterative): BJACOBI. 
  !KSP_TYPE fgmres      ! default: BCGS. NOT much difference 
END

NEWTON_SOLVER
  ATOL 1.d-20
  RTOL 1.d-15
  STOL 1.d-10
  ITOL 1.d-10
  !NUMERICAL_JACOBIAN
END
END

#=========================== discretization ===================================
GRID
  TYPE structured
  ORIGIN 0.d0 0.d0 0.d0
  NXYZ 1 1 1
  DXYZ
    1.d0
    1.d0
    10.d0
  /
END

#=========================== fluid properties =================================
FLUID_PROPERTY
  PHASE LIQUID
  DIFFUSION_COEFFICIENT 0.d-9
END

FLUID_PROPERTY
  PHASE gas
  DIFFUSION_COEFFICIENT 0.000d-05
END

#=========================== material properties ==============================
MATERIAL_PROPERTY soil1
  ID 1
  POROSITY 0.25d0
  TORTUOSITY 1.d0
  ROCK_DENSITY 2650.d0
  THERMAL_CONDUCTIVITY_DRY 0.5d0
  THERMAL_CONDUCTIVITY_WET 2.d0
  HEAT_CAPACITY 830.d0
  CHARACTERISTIC_CURVES default
  PERMEABILITY
    PERM_X 1.d-15
    PERM_Y 1.d-15
    PERM_Z 1.d-15
  END
  
END

# == saturation / permeability functions ======================================
CHARACTERISTIC_CURVES default
  SATURATION_FUNCTION VAN_GENUCHTEN
    ALPHA 2.d-4
    M 0.25d0
    LIQUID_RESIDUAL_SATURATION 0.05d0
  END
  PERMEABILITY_FUNCTION MUALEM_VG_LIQ
    PHASE LIQUID
    M 0.25d0
    LIQUID_RESIDUAL_SATURATION 0.05d0
  END
  PERMEABILITY_FUNCTION MUALEM_VG_GAS
    PHASE GAS
    LIQUID_RESIDUAL_SATURATION 0.1d0
    GAS_RESIDUAL_SATURATION 0.01d0
    M 0.5d0
  END
  
END

#=========================== output options ===================================
OUTPUT
  PERIODIC_OBSERVATION TIME 1.0 d
  #SCREEN PERIODIC 10
  #PERIODIC TIMESTEP 1
  #FORMAT HDF5
