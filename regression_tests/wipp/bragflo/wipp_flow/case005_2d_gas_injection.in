SOURCE_SINK well
  FLOW_CONDITION well
  REGION injection_point
END

!=========================== solver options ===================================
NEWTON_SOLVER FLOW
!  RTOL 1.d-08
!  ATOL 1.d-08
!  STOL 1.d-30       ! Turn off STOL criteria
  MAXIT 20          ! Maximum newton iteration number
END

LINEAR_SOLVER FLOW
!  SOLVER DIRECT
END

!=========================== output options ===================================
skip
OUTPUT
  TIMES s 1.d2 1.d3 1.d4 1.d5 1.d6   ! USER REQUESTED PRINTOUT TIMES
  FORMAT HDF5
  VARIABLES
    LIQUID_PRESSURE
    GAS_PRESSURE
    GAS_SATURATION
    GAS_DENSITY
  /
END
noskip

!=========================== times ============================================
TIME
  FINAL_TIME 1.d7 s
  INITIAL_TIMESTEP_SIZE 1.d0 s
  MAXIMUM_TIMESTEP_SIZE 1000.d0 s
  MAXIMUM_TIMESTEP_SIZE 1.d5 y at 5000 s  ! DT_MAX in BF: BF units are all in seconds
END

!==============================================================================
END_SUBSURFACE
