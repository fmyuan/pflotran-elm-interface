

!=========================== flow mode ========================================

SIMULATION
  SIMULATION_TYPE SUBSURFACE
  
  PROCESS_MODELS
 
    SUBSURFACE_FLOW flow
      MODE WIPP_FLOW
      OPTIONS
        ALLOW_NEGATIVE_GAS_PRESSURE
        HARMONIC_PERMEABILITY_ONLY
        DEFAULT_ALPHA
        GAS_COMPONENT_FORMULA_WEIGHT     2.01588D0 ! H2 kg/kmol
      END
    END
  
    #SUBSURFACE_TRANSPORT  nw_trans
    #  MODE NWT
    #END

    WELL_MODEL well01
      OPTIONS
        TYPE WIPP_DARCY
      /
    END
    
  END
END

!==============================================================================

SUBSURFACE

#=========================== numerical methods ================================
NUMERICAL_METHODS FLOW

  TIMESTEPPER
    GAS_SAT_CHANGE_TS_GOVERNOR       3.d-1
    GAS_SAT_GOV_SWITCH_ABS_TO_REL    1.d0
    LIQ_PRES_CHANGE_TS_GOVERNOR      5.d5   ! Pa
  /

  NEWTON_SOLVER
    LIQUID_RESIDUAL_INFINITY_TOL     1.d-6
    GAS_RESIDUAL_INFINITY_TOL        1.d-6
    MAX_ALLOW_REL_LIQ_PRES_CHANG_NI  1.d-5
    MAX_ALLOW_REL_GAS_SAT_CHANGE_NI  1.d-4
    REL_GAS_SATURATION_PERTURBATION  1.d-8
    REL_LIQ_PRESSURE_PERTURBATION    1.d-8
    MIN_GAS_SATURATION_PERTURBATION  1.d-10
    MIN_LIQ_PRESSURE_PERTURBATION    1.d-2
    GAS_SAT_THRESH_FORCE_EXTRA_NI    1.d-3
    GAS_SAT_THRESH_FORCE_TS_CUT      0.20d0
    MAX_ALLOW_GAS_SAT_CHANGE_TS      1.0d0
    MIN_LIQ_PRES_FORCE_TS_CUT       -1.d8   ! Pa
    MAX_ALLOW_LIQ_PRES_CHANGE_TS     1.d7   ! Pa
    JACOBIAN_PRESSURE_DERIV_SCALE    1.d7
    SCALE_JACOBIAN
  /

  LINEAR_SOLVER
    SOLVER DIRECT
  /

END

skip
NUMERICAL_METHODS TRANSPORT

  TIMESTEPPER
    NUM_STEPS_AFTER_TS_CUT 2
    DT_FACTOR 2.0 2.0 1.8 1.6 1.4 1.4 1.3 1.3 1.2 1.2 1.1 1.1
  /

  NEWTON_SOLVER
    ATOL 1.D-50
    STOL 1.D-5
    RTOL 1.D-5
    DTOL 1.D4 
    NWT_ITOL_RELATIVE_UPDATE 
      AM241L  1.0d-1
      PU239L  1.0d-1
    /

    NWT_ITOL_SCALED_RESIDUAL
      AM241L  1.D-3
      PU239L  1.D-3
    /
    NWT_ITOL_ABSOLUTE_RESIDUAL
      AM241L  1.D-10
      PU239L  1.D-7
    /
    #VERBOSE_LOGGING
    #PRINT_DETAILED_CONVERGENCE
    MAXIMUM_NUMBER_OF_ITERATIONS 12
  /

  LINEAR_SOLVER
    SOLVER DIRECT
  /

END
noskip
!==============================================================================

!=========================== REGRESSION =======================================

REGRESSION
  CELL_IDS
    3
  /
END

!=========================== discretization ===================================

GRID
  GRAVITY 0.0d0  -9.80665d+00  0.0d0  ! use y as the vertical direction
  TYPE structured
  NXYZ 3 5 1
  DXYZ
     100.0d0 
     100.0d0 
     100.0d0
  /
END

!=========================== wellbore model ===================================

# All ordering begins at the bottom of the hole and goes towards 
# the top of the hole.
WELLBORE_MODEL
  #SINGLE_PHASE
  TWO_PHASE
  #LIQUID_MOBILITY 1.0d-8     # mobility is kr/visc
  #GAS_MOBILITY 1.0d-3        # mobility is kr/visc
  WELL_GRID
    NUMBER_OF_SEGMENTS 3
    WELL_INDEX 1.0d0 1.0d0 1.0d0
    TOP_OF_HOLE    150.0d0 500.0d0 50.0d0  # [x,y,z] in meters
    BOTTOM_OF_HOLE 150.0d0 200.0d0 50.0d0  # [x,y,z] in meters
  /
  # The number of values listed in the WELL sub-block needs to match the
  # number of segments. If all segments have the same value, then you can
  # provide a single value and it will be applied to all segments.
  WELL
    DIAMETER 1.2d0 1.3d0 1.4d0
    FRICTION_COEFFICIENT 1.00001
    SKIN_FACTOR 0.d0
    WELL_INDEX_MODEL PEACEMAN_ISO
  /
  # Either BOTTOM_OF_HOLE or TOP_OF_HOLE sub-blocks need to be specified.
  # You can only specify a PRESSURE [Pa] or a FLUX.
  # For FLUX, is it going to be a MASS_FLUX or a VOLUMETRIC_FLUX????
  WELL_BOUNDARY_CONDITIONS
    BOTTOM_OF_HOLE
      #PRESSURE 4.8d6 # [Pa]
      PRESSURE_SET_BY_RESERVOIR
      FLUX
    /
    #TOP_OF_HOLE
    #  PRESSURE 0.0d0 # [Pa]
    #  FLUX
    #/
  /
END

!=========================== fluid properties =================================

FLUID_PROPERTY
  DIFFUSION_COEFFICIENT 0.d0
END

EOS WATER
  DENSITY EXPONENTIAL 1.2200E+03 101325.d0 3.1000E-10 ! ref_dens ref_pres compres
  VISCOSITY CONSTANT 2.10000E-03
END

EOS GAS
  DENSITY IDEAL
  VISCOSITY CONSTANT 8.93389E-06
END

!=========================== material properties ==============================

MATERIAL_PROPERTY REPO
  ID 3
  CHARACTERISTIC_CURVES cc_KRP4
  POROSITY 3.000000D-01
  SOIL_COMPRESSIBILITY_FUNCTION POROSITY_EXPONENTIAL
  POROSITY_COMPRESSIBILITY 1.0D-09
  SOIL_REFERENCE_PRESSURE INITIAL_PRESSURE
  PERMEABILITY
    PERM_ISO 1.0D-12
  /
END

!=========================== saturation functions =============================

CHARACTERISTIC_CURVES cc_KRP4
  SATURATION_FUNCTION BRAGFLO_KRP4
    LIQUID_RESIDUAL_SATURATION 1.000000E-06
    GAS_RESIDUAL_SATURATION    1.000000E-01
    LAMBDA 7.000000E-01
    MAX_CAPILLARY_PRESSURE 1.00000d+08
    KPC 1
    PCT_A 0.000000E+00
    PCT_EXP 0.000000E+00
  /
  PERMEABILITY_FUNCTION BRAGFLO_KRP4_LIQ
    LIQUID_RESIDUAL_SATURATION 1.000000E-06
    GAS_RESIDUAL_SATURATION    1.000000E-01
    LAMBDA 7.000000E-01
  /
  PERMEABILITY_FUNCTION BRAGFLO_KRP4_GAS
    LIQUID_RESIDUAL_SATURATION 1.000000E-06
    GAS_RESIDUAL_SATURATION    1.000000E-01
    LAMBDA 7.000000E-01
  /
END

!=========================== regions ==========================================

REGION rgs_repo
  COORDINATES
    -1.d20 -1.d20 -1.d20
     1.d20  1.d20  1.d20
  /
END

REGION bc_face_west
  FACE WEST
  COORDINATES
    0.d0 0.d0   0.d0
    0.d0 500.d0 100.0d0
  /
END

REGION bc_face_east
  FACE EAST
  COORDINATES
    300.d0 0.d0   0.d0
    300.d0 500.d0 100.0d0
  /
END

!=========================== stratigraphy couplers ============================

STRATA
  MATERIAL REPO
  REGION rgs_repo
END

!=========================== flow conditions ==================================

FLOW_CONDITION bc
  TYPE
    LIQUID_PRESSURE DIRICHLET
    LIQUID_SATURATION DIRICHLET
  /
  LIQUID_PRESSURE 2.0d6 Pa
  LIQUID_SATURATION 1.0d0
END

FLOW_CONDITION initial
  TYPE
    LIQUID_PRESSURE DIRICHLET
    LIQUID_SATURATION DIRICHLET
  /
  LIQUID_PRESSURE 1.d6 Pa
  LIQUID_SATURATION 1.0d0
END

!=========================== transport conditions =============================
skip
TRANSPORT_CONDITION transport_initial_condition
  TYPE dirichlet_zero_gradient
  CONSTRAINT_LIST
    ! time[s]    constraint
    0.0d0      constraint_chemistry_initial
  /
END

TRANSPORT_CONDITION transport_bc_condition
  TYPE dirichlet_zero_gradient
  CONSTRAINT_LIST
    ! time[s]    constraint
    0.0d0      constraint_chemistry_bc
  /
END
noskip
!=========================== condition couplers ===============================

INITIAL_CONDITION
  FLOW_CONDITION initial
  TRANSPORT_CONDITION transport_initial_condition
  REGION rgs_repo
END

BOUNDARY_CONDITION
  FLOW_CONDITION bc
  TRANSPORT_CONDITION transport_bc_condition
  REGION bc_face_west
END

BOUNDARY_CONDITION
  FLOW_CONDITION initial
  TRANSPORT_CONDITION transport_initial_condition
  REGION bc_face_east
END

!=========================== solver options ===================================



!=========================== times ============================================

TIME
  FINAL_TIME 55.d0 yr
  INITIAL_TIMESTEP_SIZE 1.d0 yr
  MAXIMUM_TIMESTEP_SIZE 55.d0 yr
END

!=========================== output options ===================================

OBSERVATION
  REGION rgs_repo
END


OUTPUT 
  skip
  OBSERVATION_FILE
    NO_PRINT_INITIAL
    ! NO_PRINT_FINAL
    PERIODIC TIMESTEP 1
    VARIABLES
      VOLUME
    /
  /
  noskip
  SNAPSHOT_FILE
    PERIODIC TIMESTEP 1
    FORMAT HDF5
  /
END


!=============================== transport ====================================
skip
NUCLEAR_WASTE_CHEMISTRY

  SPECIES
    NAME                 AM241L
    SOLUBILITY           3.08531847680638d-03    # [mol/m^3-liq]
    PRECIP_MOLAR_DENSITY 38.61d3                 # [mol/m^3-mnrl] (quartz example)
    ELEMENTAL_KD         0.0d0                   # [m^3-water/m^3-bulk]
  /

  SPECIES
    NAME                      PU239L
    SOLUBILITY                5.94620667361208d-03   # [mol/m^3-liq]
    PRECIPITATE_MOLAR_DENSITY 38.61d3                # [mol/m^3-mnrl] (quartz example)
    ELEMENTAL_KD              0.0d0                  # [m^3-water/m^3-bulk]
  /                     

  OUTPUT
    ALL_SPECIES
    ALL_CONCENTRATIONS
    MINERAL_VOLUME_FRACTION
  /

END

!=========================== transport constraints ============================

CONSTRAINT constraint_chemistry_initial
  CONCENTRATIONS
   ! species_name  concentration  constraint_type 
   ! AQ is for aqueous concentration [mol/m^3-liq]
   ! Cell volume is 100.0 x 100.0 x 4.38406d1 m^3 = 438406.0 m^3

    AM241L    1.d-20    AQ  # [mol/m^3-liq]
    PU239L    1.d-20    AQ  # [mol/m^3-liq]
  /
END

CONSTRAINT constraint_chemistry_bc
  CONCENTRATIONS
   ! species_name  concentration  constraint_type 
   ! VF is for mineral volume fraction [m^3-mnrl/m^3-void]
   ! Cell volume is 100.0 x 100.0 x 4.38406d1 m^3 = 438406.0 m^3

    AM241L    1.d-8    VF  # [m^3-mnrl/m^3-void]
    PU239L    1.d-5    VF  # [m^3-mnrl/m^3-void]
  /
END
noskip

!==============================================================================
END_SUBSURFACE
!==============================================================================

